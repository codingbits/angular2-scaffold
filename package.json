{
  "name": "angualr2-scaffold",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "typings install",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean -s",
    "build": "npm run build:scripts -s && npm run build:styles -s",
    "build:scripts": "tsc",
    "build:styles": "node-sass --include-path scss client/styles/app.scss client/assets/app.css",
    "postbuild": "gulp",
    "prewatch": "npm run clean -s",
    "watch": "npm run watch:scripts & npm run watch:styles",
    "watch:scripts": "tsc -w",
    "watch:styles": "node-sass -w client/styles/ -o client/assets/",
    "start": "npm run live:reload & npm run watch & nodemon --ignore client --exec 'node' server/index.js",
    "test": "npm run test:client && npm run test:server",
    "test:server": "mocha server --recursive",
    "test:client": "tsc & karma start karma.conf.js",
    "test:e2e": "tsc & NODE_ENV=test node server/index.js & protractor protractor.config.js",
    "webdriver:update": "webdriver-manager update",
    "live:reload": "live-reload --port 9091 client/ server/",
    "docker": "npm run build && docker build -t irvui/twizzle-ui:latest .",
    "docker:start": "docker run -p 3000:3000 irvui/twizzle-ui",
    "prod": "npm run build && cd dist && npm i --production && node server/index.js"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "body-parser": "^1.15.1",
    "bootstrap-sass": "^3.3.6",
    "compression": "^1.6.2",
    "core-js": "^2.4.0",
    "ejs": "^2.4.2",
    "errorhandler": "^1.4.3",
    "es6-promise": "^3.2.1",
    "es6-shim": "^0.35.1",
    "express": "^4.13.4",
    "lodash": "^4.13.1",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-github": "^1.1.0",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.29",
    "typings": "^1.0.4",
    "verror": "^1.6.1",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "browser-sync": "^2.12.12",
    "canonical-path": "0.0.2",
    "concurrently": "^2.1.0",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.0.7",
    "gulp-sourcemaps": "^1.6.0",
    "jasmine": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "live-reload": "^1.1.0",
    "mocha": "^2.5.3",
    "node-sass": "^3.7.0",
    "nodemon": "^1.9.2",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "^3.3.0",
    "ripe": "^0.2.1",
    "should": "^9.0.0",
    "supertest": "^1.2.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}
